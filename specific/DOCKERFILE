ARG baseimage

FROM $baseimage

ARG NAVDVDURL=""
ARG VSIXURL=""
ARG IsBcSandbox="N"

ENV DatabaseServer=localhost DatabaseInstance=SQLEXPRESS DatabaseName=CRONUS IsBcSandbox=$IsBcSandbox

COPY ./buildimage.ps1 C:/RUN/buildimage.ps1

RUN .\Run\buildimage.ps1 -NAVDVDURL $NAVDVDURL -VSIXURL $VSIXURL
