ARG baseimage

FROM $baseimage

ARG NAVDVDURL=""
ARG VSIXURL=""
ARG IsBcSandbox="N"

ENV DatabaseServer=localhost DatabaseInstance=SQLEXPRESS DatabaseName=CRONUS NAVDVDURL=$NAVDVDURL VSIXURL=$VSIXURL IsBcSandbox=$IsBcSandbox

RUN .\Run\buildimage.ps1 -NAVDVDURL $NAVDVDURL -VSIXURL $VSIXURL
